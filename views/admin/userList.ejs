<main class="main-wrap">
    <%-include('../partials/adminHeader.ejs')-%>
    <section class="content-main">
        <div class="content-header">
            <h2 class="content-title">User list</h2>
        </div>
        <div class="card mb-4">
            <div class="card-body">
                <div class="table-responsive">
                    <table class="table table-hover" id="userTable">
                        <thead>
                            <tr>
                                <th>Index</th>
                                <th>user</th>
                                <th>Email</th>
                                <!-- <th>Status</th> -->
                                <th>Registered</th>
                                <th class="text-end">Action</th>
                            </tr>
                        </thead>
                        <tbody>
                            <% allUser.forEach(function(user,index) { %>
                            <tr>
                                <td><%=index+1%></td>
                                <td width="40%">
                                    <a href="#" class="itemside">
                                        <div class="left">
                                            <img src="/assets/imgs/people/avatar1.jpg" class="img-sm img-avatar" alt="Userpic">
                                        </div>
                                        <div class="info pl-3">
                                            <h6 class="mb-0 title"><%= user.firstname %></h6>
                                            <small class="text-muted"><%= user._id %></small>
                                        </div>
                                    </a>
                                </td>
                                <td><%= user.email %></td>
                                <!-- <td><span class="badge rounded-pill alert-success">Active</span></td> -->
                                <td>08.07.2020</td>
                                <% if (user.isblocked) { %>
                                    <form method="POST" action="/admin/unBlockUser/<%= user._id %>">
                                     <td> <button type="submit" class="btn btn-sm btn-success rounded font-sm mt-15">unblock</button></td>
                                    </form>
                                  <% } else { %>
                                    <form method="POST" action="/admin/blockUser/<%= user._id %>">
                                      <td><button type="submit" class="btn btn-sm btn-danger rounded font-sm mt-15">Block</button></td>
                                    </form>
                                  <% } %>
                            </tr>
                            <% }); %>
                        </tbody>
                    </table>
                </div>
            </div>
        </div>
       
    </section>
    <%-include('../partials/adminFooter.ejs')%>

    


   
   

